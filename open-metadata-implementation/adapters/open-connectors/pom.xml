    <parent>
        <artifactId>adapters</artifactId>
        <groupId>org.odpi.egeria</groupId>
        <version>3.13-SNAPSHOT</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>

    <scm>
        <connection>scm:git:git://github.com/odpi/egeria.git</connection>
        <developerConnection>scm:git:ssh://github.com/odpi/egeria.git</developerConnection>
        <url>http://github.com/odpi/egeria/tree/master</url>
    </scm>

    <name>Open Connectors</name>
    <description>
        Open Connectors are connectors that support the
        Open Connector Framework (OCF) - see the open-connector-framework module.
        This is the principle (preferred) mechanism for supporting pluggable
        components in the open metadata implementation.
    </description>

    <artifactId>open-connectors</artifactId>

    <packaging>pom</packaging>
    <modules>
        <module>event-bus-connectors</module>
        <module>repository-services-connectors</module>
        <module>data-store-connectors</module>
        <module>integration-connectors</module>
        <module>governance-daemon-connectors</module>
        <module>discovery-service-connectors</module>
        <module>governance-action-connectors</module>
        <module>dynamic-archiver-connectors</module>
        <module>configuration-store-connectors</module>
        <module>rest-client-connectors</module>
        <module>connector-configuration-factory</module>
    </modules>

</project>
