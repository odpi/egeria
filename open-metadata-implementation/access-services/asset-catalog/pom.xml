<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <parent>
        <artifactId>access-services</artifactId>
        <groupId>org.odpi.egeria</groupId>
        <version>3.13-SNAPSHOT</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>

    <scm>
        <connection>scm:git:git://github.com/odpi/egeria.git</connection>
        <developerConnection>scm:git:ssh://github.com/odpi/egeria.git</developerConnection>
        <url>http://github.com/odpi/egeria/tree/master</url>
    </scm>

    <name>Asset Catalog OMAS</name>
    <description>
        The Asset Catalog Open Metadata Access Service (OMAS)
        provides the ability to search for assets stored in the
        open metadata repositories.
    </description>

    <artifactId>asset-catalog</artifactId>

    <!-- POM packaging means that this module has sub-modules -->
    <packaging>pom</packaging>
    <modules>
        <module>asset-catalog-api</module>
        <module>asset-catalog-client</module>
        <module>asset-catalog-server</module>
        <module>asset-catalog-spring</module>
        <module>asset-catalog-topic-connector</module>
    </modules>

</project>
