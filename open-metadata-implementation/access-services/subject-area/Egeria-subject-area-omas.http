# @name Update Glossary
PUT {{baseURL}}/servers/{{server}}/open-metadata/access-services/subject-area/users/{{user}}/glossaries/{{glossaryGUID}}
Content-Type: application/json

{
    "class" : "Glossary",
    "qualifiedName" : "glossaryTime",
    "name" : "XY",
    "effectiveFromTime" : 77777777,
    "effectiveToTime" : 9999999999
}

