    <modelVersion>4.0.0</modelVersion>

    <name>Asset Consumer OMAS</name>
    <description>
        The Asset Consumer Open Metadata Access Services (OMAS) provides services for an application
        accessing assets such as data stores, APIs or functions such as analytical services.
    </description>

    <artifactId>asset-consumer</artifactId>
    <packaging>pom</packaging>
    <modules>
        <module>asset-consumer-api</module>
        <module>asset-consumer-client</module>
        <module>asset-consumer-server</module>
        <module>asset-consumer-spring</module>
    </modules>

    <!-- Asset Consumer is built on Connected Asset -->
    <dependencies>
        <dependency>
            <groupId>org.odpi.egeria</groupId>
            <artifactId>connected-asset-client</artifactId>
            <version>${open-metadata.version}</version>
        </dependency>
    </dependencies>

</project>