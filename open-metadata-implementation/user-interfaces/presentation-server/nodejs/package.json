{
  "name": "presentation-server",
  "version": "1.0.0",
  "description": "Egeria Presentation Server.",
  "main": "src/server/index.js",
  "scripts": {
    "build:dev": "webpack --mode development --devtool inline-source-map",
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "start:dev": "npm run build:dev && node src/server/index.js",
    "debug": "npm run build && node --nolazy --inspect-brk=9229 src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "devold": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "webpack-dev-server --open"
  },
  "author": "David Radley",
  "license": "Apache-2.0",
